		<http:request-connection host="${http.request.host}" port="${http.request.port}" />
	</http:request-config>
	<configuration doc:name="Configuration" doc:id="26eb655e-72c0-4004-bece-46a5d4966530" defaultErrorHandler-ref="flow-level-error-handler" />
	
</mule>
